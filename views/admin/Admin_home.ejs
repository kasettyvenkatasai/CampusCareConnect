}
        
    </script>
    <script>
        let xhr =new XMLHttpRequest()
       xhr.onreadystatechange = function(){
        if (xhr.readyState === 4 && xhr.status === 200) {
            
            const data = JSON.parse(xhr.responseText)
            const email = data.email
            document.getElementById('mai').innerHTML = email
            document.getElementById('students').innerHTML = data.students.length
            document.getElementById('doctors').innerHTML = data.doctors.length
            document.getElementById('comapp').innerHTML = data.comapp.length
            document.getElementById('upcomapp').innerHTML = data.upcomapp.length
        }}
        xhr.open('GET', '/getadmindetails', true);
        xhr.send();
        console.log('sent')
    </script>
